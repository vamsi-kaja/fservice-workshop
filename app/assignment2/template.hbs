<div class="tabs">
  <section class="tabs-items">
    {{#each @model as |tab|}}
      <a href="#{{tab.name}}" class="tabs-item {{if (eq tab.name this.selectedTab) 'active'}}"
        {{on "click" (fn this.selectTab tab.name)}}>
        {{tab.label}}
      </a>
    {{/each}}
  </section>
  <div class="tabs-content">
    {{#each @model as |tab|}}
      <div class="tabs-pane {{if (eq tab.name this.selectedTab) 'active'}}">
